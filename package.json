{
  "name": "tg-backoffice",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "tsc -w -p ./src/server & nodemon --inspect --delay 5 ./dist/server/server.js",
    "dev:client": "ng serve --disable-host-check --open --serve-path / --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "npm-run-all -p build:*",
    "build:server": "tsc -p ./src/server",
    "build:client": "ng build --prod --extract-css=true --build-optimizer=true --vendor-chunk=true --stats-json --output-path=dist/client --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-translations": "ngx-translate-extract --input ./src/client --output ./src/client/assets/i18n/*.json --clean --sort --format json --marker TRANSLATE"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.1.0",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/elements": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.1.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/pwa": "^0.6.5",
    "@angular/router": "^6.0.3",
    "@angular/service-worker": "^6.0.3",
    "@ngrx/effects": "latest",
    "@ngrx/router-store": "latest",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "latest",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/axios": "^0.14.0",
    "@types/bunyan": "^1.8.4",
    "@types/express": "^4.11.1",
    "@types/googlemaps": "^3.30.8",
    "@types/lodash": "^4.14.116",
    "aws-sdk": "^2.250.1",
    "body-parser": "^1.18.2",
    "bunyan": "latest",
    "bunyan-newrelic-stream": "^1.2.0",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.4",
    "cors": "^2.8.4",
    "document-register-element": "^1.7.2",
    "express": "^4.16.3",
    "express-healthcheck": "^0.1.0",
    "http-status": "^1.1.2",
    "jsonwebtoken": "^8.3.0",
    "mongodb": "^3.0.7",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "ngrx-store-freeze": "latest",
    "ngrx-store-logger": "latest",
    "ngx-color-picker": "^6.7.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typeorm": "^0.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.5",
    "@angular/cli": "~6.0.7",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^9.6.5",
    "autoprefixer": "^7.2.3",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "ts-node": "~5.0.1",
    "tslint": "~5.11.0",
    "typescript": "~2.7.2"
  }
}
